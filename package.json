{
  "name": "@nsisodiya/egov-ui-react-kit",
  "version": "1.0.0",
  "description": "UI React Kit for the EGov Project",
  "main": "dist/EGovReactKit.js",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "precommit": "npm run eslint",
    "devserver": "webpack-dev-server --config webpack.dev.js",
    "productionBuild": "npm run prettier && npm run build",
    "build": "webpack --config webpack.prod.js",
    "generateStates": "webpack --json --config webpack.prod.js> state.json",
    "prettier": "prettier --write \"src/**/*.js\"",
    "eslint": "eslint -c .eslintrc src",
    "publishPackage": "np --any-branch ",
    "createGitLog": "git log --graph --pretty=format:'%h%  -   <%an> -   %s (%cD)' --abbrev-commit | head -n50 > ./dist/log.txt",
    "detectCopyPaste": "jscpd"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nsisodiya/egov-ui-react-kit.gs"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/nsisodiya/egov-ui-react-kit/issues"
  },
  "homepage": "https://github.com/nsisodiya/egov-ui-react-kit#readme",
  "peerDependencies": {
    "prop-types": ">=15.5.8",
    "react": ">=0.14.0",
    "react-dom": ">=0.14.0",
    "material-ui": ">=0.18.1",
    "styled-components": ">=2.2.0",
    "react-json-viewer": ">=1.7.1"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-core": "6.26.0",
    "babel-eslint": "8.0.3",
    "babel-loader": "7.1.2",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-preset-env": "1.6.1",
    "babel-preset-react": "6.24.1",
    "copy-webpack-plugin": "4.3.0",
    "css-loader": "0.28.7",
    "eslint": "4.13.1",
    "eslint-loader": "1.9.0",
    "eslint-plugin-babel": "4.1.2",
    "eslint-plugin-react": "7.5.1",
    "husky": "0.14.3",
    "prettier": "1.9.2",
    "style-loader": "0.19.1",
    "webpack": "3.10.0",
    "webpack-dev-server": "2.9.7",
    "prop-types": "15.6.0",
    "react": "16.2.0",
    "react-dom": "16.2.0",
    "material-ui": "0.20.0",
    "normalize.css": "7.0.0",
    "react-json-viewer": "1.7.1",
    "styled-components": "2.2.4"
  }
}
