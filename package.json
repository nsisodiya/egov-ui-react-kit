{
  "name": "@nsisodiya/egov-ui-react-kit",
  "version": "1.0.0",
  "description": "UI React Kit for the EGov Project",
  "main": "index.js",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "precommit": "npm run eslint",
    "test": "echo \"Error: no test specified\"",
    "publishPackage": "np --any-branch ",
    "eslint": "eslint -c .eslintrc src",
    "createGitLog": "git log --graph --pretty=format:'%h%  -   <%an> -   %s (%cD)' --abbrev-commit | head -n50 > ./dist/log.txt",
    "devserver": "webpack-dev-server --content-base ./dist --port 8123 --colors --history-api-fallback",
    "productionBuild": "cross-env NODE_ENV=production webpack --config webpack.config.js --progress --colors",
    "watch": "webpack --config webpack.config.js --watch",
    "generateStates": "webpack --profile --json > stats.json",
    "prettierJS": "??",
    "prettierCSS": "??",
    "detectCopyPaste": "jscpd"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nsisodiya/egov-ui-react-kit.gs"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/nsisodiya/egov-ui-react-kit/issues"
  },
  "homepage": "https://github.com/nsisodiya/egov-ui-react-kit#readme",
  "devDependencies": {
    "babel-core": "6.26.0",
    "babel-loader": "7.1.2",
    "babel-preset-env": "1.6.1",
    "copy-webpack-plugin": "4.2.3",
    "eslint": "4.12.1",
    "eslint-loader": "1.9.0",
    "eslint-plugin-react": "7.5.1",
    "husky": "0.14.3",
    "prettier": "1.9.1",
    "webpack": "3.10.0",
    "webpack-dev-server": "2.9.5"
  },
  "dependencies": {
    "react": "16.2.0",
    "react-dom": "16.2.0"
  }
}
